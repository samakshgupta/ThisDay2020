<div class="row">
	<div class="col-md-8 mx-auto mt-5">
		<div class="bgc-white bd bdrs-3 p-20 mB-20">
			<h3 class="c-grey-900 mB-20 white">Answers By List</h3>

			<div id="dataTable_wrapper" class="dataTable_wrapper">
						<table id="dataTable" class="table table-striped table-bordered dataTable" cellspacing="0" width="100%" role="grid" aria-describedby="dataTable_info" style="width: 100%;">
							<thead>
								<tr role="row">
									<th class="sorting_desc white" aria-sort="descending" tabindex="0" aria-controls="dataTable" rowspan="1" colspan="1" style="width: 198px;">Question</th>
								<th class="sorting_desc white" aria-sort="descending" tabindex="0" aria-controls="dataTable" rowspan="1" colspan="1" style="width: 198px;">Answer</th>
                                <th class="sorting_desc white" aria-sort="descending" tabindex="0" aria-controls="dataTable" rowspan="1" colspan="1" style="width: 198px;">Created At</th>
								</tr>
							</thead>
							<tfoot>
								<tr>
									<th class="sorting_desc white" aria-sort="descending" tabindex="0" aria-controls="dataTable" rowspan="1" colspan="1" style="width: 198px;">Question</th>
									<th class="sorting_desc white" aria-sort="descending" tabindex="0" aria-controls="dataTable" rowspan="1" colspan="1" style="width: 198px;">Answer</th>
                                    <th class="sorting_desc white" aria-sort="descending" tabindex="0" aria-controls="dataTable" rowspan="1" colspan="1" style="width: 198px;">Created At</th>
								</tr>
							</tfoot>

							<tbody>
								{{#each answers}}
								<tr>
									<td class = "white">
                                        {{question}}
                                    </td>
									<td class = "white">
                                        {{answer}}
                                    </td>
                                    <td class = "white">
                                        {{formatDate createdAt "DD-MM-YYYY"}}
                                    </td>
								</tr>
								{{/each}}
							</tbody>
						</table>
			</div>
		</div>

        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
	</div>
</div>
